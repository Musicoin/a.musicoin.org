<script>
  $( document ).ready(function() {
    $(document).on('click', '.audio-track', function() {
      var item = $(this);
      parent.document.getElementById("playerFrame").contentWindow.audioPlayer.playItem(this,
        item.attr("audioUrl"),
        item.attr("trackTitle"),
        item.attr("artistName"),
        item.attr("image"),
        item.attr("playCount"),
        item.attr("tipCount"),
        item.attr("artistProfileAddress")
      )
    });
  });
</script>
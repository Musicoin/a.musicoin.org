<% if (typeof license != "undefined") { %>
<!-- Twitter card -->
<meta name="twitter:card" content="summary">
<meta name="twitter:site" content="@musicoins">
<meta name="twitter:title" content="<%=license.title%> by <%=license.artistName%>">
<meta name="twitter:description" content="Listen to <%=license.artistName%> and more amazing artists on musicoin.org">
<meta name="twitter:image" content="https://musicoin.org<%=license.image%>">
<meta name="description" content="Musicoin is the pioneer of connecting the blockchain with the Music industry. Powered by Smart  Contracts and a PPV model, Musicoin pays artists 100% of the streams they receive.">

<!-- Open Graph -->
<meta property="og:image" content="https://musicoin.org<%=license.image%>">
<meta property="og:description" content="<%=license.description || "Find this track and other amazing tracks on musicoin.org"%>">
<meta property="og:type" content="website">
<meta property="og:url" content="https://musicoin.org/nav/track/<%=license.address%>">
<meta property="og:title" content="<%=license.title%> by <%=license.artistName%>">
<meta property="og:site_name" content="Musicoin"/>
<meta property="fb:app_id" content="1845789109028247"/>
<% } %>
